<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity BoxItem.
    -->
    <changeSet id="20190707112831-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="box_id"
                                 baseTableName="box_item"
                                 constraintName="fk_box_item_box_id"
                                 referencedColumnNames="id"
                                 referencedTableName="box"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="box_item"
                                 constraintName="fk_box_item_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item"/>

    </changeSet>
</databaseChangeLog>
